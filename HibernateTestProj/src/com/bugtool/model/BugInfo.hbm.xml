<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.bugtool.model.BugInfo" table="BUG_INFO">
        <id name="id" type="java.lang.Integer">
            <column name="ID" />
            <generator class="increment" />
        </id>
        <property name="steps" type="java.lang.String">
            <column name="STEPS" />
        </property>
        <property name="developerEffort" type="java.lang.String">
            <column name="DEVELOPER_EFFORT" />
        </property>
        <property name="description" type="java.lang.String">
            <column name="DESCRIPTION" />
        </property>
        <property name="rootcause" type="java.lang.String">
            <column name="ROOTCAUSE" />
        </property>
        <property name="solution" type="java.lang.String">
            <column name="SOLUTION" />
        </property>
        <property name="workaround" type="java.lang.String">
            <column name="WORKAROUND" />
        </property>
        <property name="technicalLearning" type="java.lang.String">
            <column name="TECHNICAL_LEARNING" />
        </property>
        <property name="domainLearning" type="java.lang.String">
            <column name="DOMAIN_LEARNING" />
        </property>
        <property name="status" type="java.lang.String">
            <column name="STATUS" />
        </property>
        <property name="envInfo" type="java.lang.String">
            <column name="ENV_INFO" />
        </property>
        <property name="userId" type="java.lang.Integer">
            <column name="USER_ID" />
        </property>
        <property name="supportFile" type="binary">
            <column name="SUPPORT_FILE" />
        </property>
        
    </class>
</hibernate-mapping>
