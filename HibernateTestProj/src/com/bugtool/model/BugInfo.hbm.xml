<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.bugtool.model.BugInfo" table="BUGINFO">
        <id name="id" type="java.lang.Integer">
            <column name="ID" />
            <generator class="assigned" />
        </id>
        <property name="steps" type="java.lang.String">
            <column name="STEPS" />
        </property>
        <property name="developerEffort" type="java.lang.String">
            <column name="DEVELOPEREFFORT" />
        </property>
        <property name="description" type="java.lang.String">
            <column name="DESCRIPTION" />
        </property>
        <property name="rootcause" type="java.lang.String">
            <column name="ROOTCAUSE" />
        </property>
        <property name="solution" type="java.lang.String">
            <column name="SOLUTION" />
        </property>
        <property name="workaround" type="java.lang.String">
            <column name="WORKAROUND" />
        </property>
        <property name="technicalLearning" type="java.lang.String">
            <column name="TECHNICALLEARNING" />
        </property>
        <property name="domainLearning" type="java.lang.String">
            <column name="DOMAINLEARNING" />
        </property>
        <property name="status" type="java.lang.String">
            <column name="STATUS" />
        </property>
        <property name="envInfo" type="java.lang.String">
            <column name="ENVINFO" />
        </property>
        <property name="userId" type="java.math.BigDecimal">
            <column name="USERID" />
        </property>
    </class>
</hibernate-mapping>
